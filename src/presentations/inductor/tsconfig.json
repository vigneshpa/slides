{
  "compilerOptions": {
    "sourceMap": false,
    "target": "ES2019",
    "module": "ES6",
    "moduleResolution": "node",
    "importsNotUsedAsValues": "error",
    "strict": true,
    "lib": [
      "esnext",
      "DOM",
      "DOM.Iterable"
    ],
    "removeComments": true,
    "inlineSourceMap": false,
    "esModuleInterop": false,
  },
  "include": [
    "./**/*"
  ],
}